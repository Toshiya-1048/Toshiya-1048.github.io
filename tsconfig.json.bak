{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "es2020", "es2016", "es2017"],
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "baseUrl": "src",
    "paths": {
      "*": ["*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "bk"]
}
